[Desktop Entry]
Type=Service
Actions=ChangeOwner;
Icon=ks-owner
MimeType=all/all;
X-KDE-ServiceTypes=KonqPopupMenu/Plugin,inode/directory
X-KDE-StartupNotify=false
X-KDE-Submenu=KDE-Services
X-KDE-Submenu[x-test]=xxKDE-Servicesxx

[Desktop Action ChangeOwner]
Icon=ks-owner
Name=Change Owner Here
Exec=kdesu -i ks-owner --noignorebutton -d ~/.local/share/applications/Dolphin_Tools-change-owner.sh %F
