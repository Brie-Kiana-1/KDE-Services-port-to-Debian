Icon=/usr/share/icons/hicolor/512x512/apps/ks-checksum.png
Name=Show SHA512sum ISO Image
Name[es]=Mostrar SHA512sum Imagen ISO
Name[ru]=Показ kонтрольная сумма SHA512 ISO-образа
Name[uk]=Покажіть kонтрольна сума SHA512 ISO-образу
Name[x-test]=xxShow SHA512sum ISO Imagexx

[Desktop Action MountCD-Image]
Exec=/usr/share/applications/iso_manager-mount-image.sh %F
Icon=/usr/share/icons/hicolor/512x512/apps/ks-media-optical-mount.png
Name=Mount ISO Image
Name[es]=Montar Imagen ISO
Name[ru]=Смонтировать ISO-образ
Name[uk]=Змонтувати ISO-образ
Name[x-test]=xxMount ISO Imagexx

[Desktop Action UnmountCD-Image]
Exec=fuser -k %f; fusermount -u "$(ls "%f"|sed 's/.iso$//')"; rm -fr "$(ls "%f"|sed 's/.iso$//')"; kdialog --icon=/usr/share/icons/hicolor/512x512/apps/ks-media-optical-umount.png --title="Unmount ISO Image" --passivepopup="[Finished] $(basename %f) unmounted."
Icon=/usr/share/icons/hicolor/512x512/apps/ks-media-optical-umount.png
Name=Unmount ISO Image
Name[es]=Desmontar Imagen ISO
Name[ru]=Размонтировать ISO-образ
Name[uk]=Розмонтувати ISO-образ
Name[x-test]=xxUnmount ISO Imagexx
